package Cotton::Node {
  allow Cotton;

  use SPVM::Hash;
  use SPVM::StringList;
  
  has first : Cotton::Node;
  has last : Cotton::Node;
  has sibparent : Cotton::Node;
  has has_sibling : byte;
  has tag : string;
  has id : string;
  has style : SPVM::Hash;
  has classes : string[];
  has text : string;
  
  sub new : Cotton::Node () {
    return new Cotton::Node;
  }
}
