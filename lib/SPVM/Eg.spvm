  static method tag_ : Eg::Node ($tag_name : string, $attributes : object[] = undef, $child_nodes_or_text : object of Eg::Node[]|string = undef) {
    
    my $child_nodes = (Eg::Node[])undef;
    if ($child_nodes_or_text) {
      if ($child_nodes_or_text is_type string) {
        $child_nodes = [Eg::Node->new_text_node((string)$child_nodes_or_text)];
      }
      elsif ($child_nodes_or_text is_type Eg::Node[]) {
        $child_nodes = (Eg::Node[])$child_nodes_or_text;
      }
      else {
        die "The type of \$child_nodes_or_text must be Eg::Node or string.";
      }
    }
    
    my $new_element_node = Eg::Node->new_element_node($tag_name, $attributes, $child_nodes);
    
    return $new_element_node;
  }
  
  static method a_ : Eg::Node ($attributes : object[] = undef, $child_nodes_or_text : object of Eg::Node[]|string = undef) {
    return &tag_("a", $attributes, $child_nodes_or_text);
  }
  
