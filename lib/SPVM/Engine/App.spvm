class Engine::App {
  use List;
  use Eg::Node;
  use Engine::Runtime;
  
  # Fields
  has runtime : rw Engine::Runtime;
  
  has name : rw string;
  
  has document : rw Eg::Node;
  
  static method new : Engine::App() {
    my $self = new Engine::App;
    
    return $self;
  }
}
