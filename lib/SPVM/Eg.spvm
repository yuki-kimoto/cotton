  static method new_document : Eg::Document ($root_node : Eg::Node) {
    unless ($root_node) {
      die "\$root_node must be defined.";
    }
    
    unless ($root_node->{tag_name}&& $root_node->{tag_name} eq "html") {
      die "The tag name of \$root_node must be \"html\".";
    }
    
    my $new_document = Eg::Document->new;
    
    $new_document->set_html($root_node);
    
    return $new_document;
  }
  
  static method text : Eg::Node ($text : string) {
    
    my $new_text_node = Eg::Node->new_text_node($text);
    
    return $new_text_node;
  }
  
  static method tag : Eg::Node ($tag_name : string, $child_nodes_or_text : object of Eg::Node[]|string = undef) {
    
    my $child_nodes = (Eg::Node[])undef;
    if ($child_nodes_or_text) {
      if ($child_nodes_or_text is_type string) {
        $child_nodes = [Eg::Node->new_text_node((string)$child_nodes_or_text)];
      }
      elsif ($child_nodes_or_text is_type Eg::Node[]) {
        $child_nodes = (Eg::Node[])$child_nodes_or_text;
      }
      else {
        die "The type of \$child_nodes_or_text must be Eg::Node or string.";
      }
    }
    
    my $new_element_node = Eg::Node->new_element_node($tag_name, undef, $child_nodes);
    
    return $new_element_node;
  }
  
