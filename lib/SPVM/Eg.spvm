  static method html : Eg::Node ($child_nodes : Eg::Node[] = undef) {
    
    unless ($child_nodes) {
      die "\$child_nodes must be defined.";
    }
    
    my $child_nodes_length = @$child_nodes;
    
    unless ($child_nodes_length == 2) {
      die "\$child_nodes_length must be 2.";
    }
    
    unless ($child_nodes->[0]->{tag_name} eq "head") {
      die "The tag name of the 1th element of \$child_nodes must be \"head\".";
    }
    
    unless ($child_nodes->[1]->{tag_name} eq "body") {
      die "The tag name of the 1th element of \$child_nodes must be \"body\".";
    }
    
    return &tag("html", $child_nodes);
  }
  
